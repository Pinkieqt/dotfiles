xrandr --output eDP1 --off --output DP1 --off --output HDMI1 --off --output HDMI2 --primary --mode 1920x1080 --pos 0x0 --rotate normal --output VIRTUAL1 --off
xinput set-prop "  GXT 930 Mouse" "libinput Accel Speed" -0.7

setxkbmap cz &

picom &

exec dunst &

exec plank &

sh ~/.config/polybar/launch.sh

nitrogen --restore

xfce4-power-manager &

ksuperkey &

sh ~/imwheel

